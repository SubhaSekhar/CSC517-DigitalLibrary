<h1>Borrwed History</h1>

<table>
  <thead>
  <tr>
    <th scope="col">Student Name</th>
    <th scope="col">Student Email</th>
    <th scope="col">Library</th>
    <th scope="col">ISBN</th>
    <th scope="col">Status</th>
    <th scope="col">Time Checked out</th>
  </tr>
  </thead>

  <tbody>
  <% @borrow.each do |bor| %>
    <tr>
      <td><%= bor.student_name %></td>
      <td><%= bor.student_email %></td>
      <td><%= bor.library_name %></td>
      <td><%= bor.isbn%></td>
      <td><%= bor.status %></td>
      <td><%= bor.updated_at %></td>
    </tr>
  <%end %>
  </tbody>
</table>
